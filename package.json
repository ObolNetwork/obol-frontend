    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@types/jest": "^26.0.23",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^0.9.1",
