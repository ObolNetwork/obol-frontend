    "gh-pages": "^3.2.3",
    "jest": "^26.6.3",
    "prettier": "2.2.1"
  },
  "eslintConfig": {
    "rules": {
      "react/jsx-pascal-case": [
        1,
        {
          "allowAllCaps": true,
          "allowNamespace": true
        }
      ]
    }
